<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>Node</name>
      <dataType>string</dataType>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>ResetNetwork</name>
    </action>
    <action>
      <name>RemoveNodes</name>
    </action>
    <action>
      <name>AddNodes</name>
    </action>
    <action>
      <name>StopAddRemoveNodes</name>
    </action>
    <action>
      <name>SendData</name>
      <argumentList>
        <argument>
          <name>Node</name>
          <direction>in</direction>
          <relatedStateVariable>Node</relatedStateVariable>
        </argument>
        <argument>
          <name>Data</name>
          <direction>in</direction>
          <relatedStateVariable>Node</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
