<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>Target</name>
      <sendEventsAttribute>no</sendEventsAttribute>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>Status</name>
      <dataType>boolean</dataType>
      <xlogCode>DL_STATUS</xlogCode>
      <shortCode>locked</shortCode>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>PinCodes</name>
      <dataType>string</dataType>
      <shortCode>pincodes</shortCode>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_UserCode</name>
      <dataType>string</dataType>
      <logCode>DL_USERCODE</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_PinFailed</name>
      <dataType>boolean</dataType>
      <logCode>DL_PINFAILED</logCode>
      <logSeverity>1</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_CodeChanged</name>
      <dataType>int</dataType>
      <logCode>DL_CODE_CHANGED</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_LockButton</name>
      <dataType>boolean</dataType>
      <logCode>DL_LOCK_BUTTON</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_LockFailure</name>
      <dataType>boolean</dataType>
      <logCode>DL_LOCK_FAILURE</logCode>
      <logSeverity>1</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_UnauthUser</name>
      <dataType>string</dataType>
      <logCode>DL_UNAUTH_USER</logCode>
      <logSeverity>1</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_PinProgramSuccess</name>
      <dataType>string</dataType>
      <logCode>DL_PINPROG_SUCC</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_PinRemoveSuccess</name>
      <dataType>string</dataType>
      <logCode>DL_PINREMV_SUCC</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_PinProgramFail</name>
      <dataType>string</dataType>
      <logCode>DL_PINPROG_FAIL</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_LockChanged</name>
      <dataType>string</dataType>
      <logCode>DL_LOCK_CHANGED</logCode>
      <logSeverity>3</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_LowBattery</name>
      <dataType>boolean</dataType>
      <logCode>DL_LOW_BATTERY</logCode>
      <logSeverity>2</logSeverity>
    </stateVariable>
    <stateVariable sendEvents="yes" allowRepeats="yes">
      <name>sl_VeryLowBattery</name>
      <dataType>boolean</dataType>
      <logCode>DL_VERY_LOW_BATTERY</logCode>
      <logSeverity>2</logSeverity>
    </stateVariable>
    <stateVariable>
      <name>dateTime</name>
      <dataType>dateTime</dataType>
    </stateVariable>
    <stateVariable>
      <name>ui4</name>
      <dataType>ui4</dataType>
    </stateVariable>
    <stateVariable>
      <name>ui1</name>
      <dataType>ui1</dataType>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>SetTarget</name>
      <argumentList>
        <argument>
          <name>newTargetValue</name>
          <direction>in</direction>
          <relatedStateVariable>Target</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPin</name>
      <argumentList>
        <argument>
          <name>json</name>
          <direction>in</direction>
          <relatedStateVariable>PinCodes</relatedStateVariable>
          <optional/>
        </argument>
        <argument>
          <name>UserCodeName</name>
          <direction>in</direction>
          <relatedStateVariable>sl_UserCode</relatedStateVariable>
          <optional/>
        </argument>
        <argument>
          <name>newPin</name>
          <direction>in</direction>
          <relatedStateVariable>sl_UserCode</relatedStateVariable>
          <optional/>
        </argument>
        <argument>
          <name>UserCode</name>
          <direction>out</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
          <optional/>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPinValidityDate</name>
      <argumentList>
        <argument>
          <name>UserCode</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
        <argument>
          <name>StartDate</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>dateTime</relatedStateVariable>
        </argument>
        <argument>
          <name>StopDate</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>dateTime</relatedStateVariable>
        </argument>
        <argument>
          <name>Replace</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>Target</relatedStateVariable>
        </argument>
        <argument>
          <name>slotID</name>
          <direction>out</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetPinValidityWeekly</name>
      <argumentList>
        <argument>
          <name>UserCode</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
        <argument>
          <name>DayOfWeek</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
        <argument>
          <name>StartHour</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>ui1</relatedStateVariable>
        </argument>
        <argument>
          <name>StartMinute</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>ui1</relatedStateVariable>
        </argument>
        <argument>
          <name>StopHour</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>ui1</relatedStateVariable>
        </argument>
        <argument>
          <name>StopMinute</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>ui1</relatedStateVariable>
        </argument>
        <argument>
          <name>Replace</name>
          <direction>in</direction>
          <optional/>
          <relatedStateVariable>Target</relatedStateVariable>
        </argument>
        <argument>
          <name>slotID</name>
          <direction>out</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>ClearPinValidity</name>
      <argumentList>
        <argument>
          <name>UserCode</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
        <argument>
          <name>slotID</name>
          <direction>in</direction>
          <relatedStateVariable>ui4</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>ClearPin</name>
      <argumentList>
        <argument>
          <name>UserCode</name>
          <direction>in</direction>
          <relatedStateVariable>sl_UserCode</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>RenamePin</name>
      <argumentList>
        <argument>
          <name>UserCode</name>
          <direction>in</direction>
          <relatedStateVariable>sl_UserCode</relatedStateVariable>
        </argument>
        <argument>
          <name>UserCodeName</name>
          <direction>in</direction>
          <relatedStateVariable>sl_UserCode</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
